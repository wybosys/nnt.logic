{
    "config": {
        "sidexpire": 86400,
        "cidexpire": 600,
        "cache": "cache",
        "https": false,
        "deskey": "joi>:90",
        "cluster": 1
    },
    "server": [
        {
            "id": "sample",
            "listen": "*",
            "port": 8090,
            "entry": "app.SampleRest",
            "enable": "local",
            "router": {
                "nnt.server.apidoc.Router": {
                    "export": {
                        "router": [
                            "entry://app/samplerest"
                        ],
                        "model": [
                            "entry://app/samplerest"
                        ]
                    }
                }
            }
        },
        {
            "id": "sample",
            "listen": "*",
            "port": 80,
            "entry": "app.SampleRest",
            "enable": "devops-develop,devops-release",
            "router": {
                "nnt.server.apidoc.Router": {
                    "export": {
                        "router": [
                            "entry://app/samplerest"
                        ],
                        "model": [
                            "entry://app/samplerest"
                        ]
                    }
                }
            }
        },
        {
            "id": "gendata",
            "entry": "nnt.server.XlsxConfig",
            "dir": "~/src/app/data",
            "output": "~/src/app/data/data.ts",
            "enable": null
        }
    ],
    "dbms": [
        {
            "id": "memdb",
            "entry": "nnt.store.KvMemory"
        }
    ],
    "logger": [
        {
            "id": "console",
            "filter": "all",
            "entry": "nnt.logger.Console",
            "enable": "local"
        }
    ]
}
