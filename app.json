{
    "config": {
        "sidexpire": 86400,
        "cidexpire": 600,
        "cache": "cache",
        "https": false,
        "deskey": "joi>:90",
        "cluster": 1
    },
    "server": [
        {
            "id": "sample",
            "listen": "*",
            "port": 8090,
            "entry": "app.Sample",
            "enable": "local",
            "router": {
                "nnt.server.apidoc.Router": {
                    "export": {
                        "router": [
                            "entry://app/sample"
                        ],
                        "model": [
                            "entry://app/sample"
                        ]
                    }
                }
            }
        },
        {
            "id": "sample",
            "listen": "*",
            "port": 80,
            "entry": "app.Sample",
            "enable": "devops,devops-release",
            "router": {
                "nnt.server.apidoc.Router": {
                    "export": {
                        "router": [
                            "entry://app/sample"
                        ],
                        "model": [
                            "entry://app/sample"
                        ]
                    }
                }
            }
        }
    ],
    "container": [
    ],
    "dbms": [
    ],
    "logger": [
        {
            "id": "console",
            "filter": "all",
            "entry": "nnt.logger.Console",
            "enable": "local"
        },
        {
            "id": "file",
            "filter": "all",
            "entry": "nnt.logger.Log4Ts",
            "config": {
                "appenders": {
                    "multi": {
                        "type": "multiFile",
                        "base": "logs/",
                        "property": "TODAY",
                        "extension": ".log"
                    }
                },
                "categories": {
                    "default": {
                        "appenders": [
                            "multi"
                        ],
                        "level": "debug"
                    }
                }
            }
        }
    ]
}
