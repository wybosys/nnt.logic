{
    "config": {
        "sidexpire": 86400,
        "cidexpire": 600,
        "cache": "cache",
        "https": false,
        "deskey": "joi>:90",
        "cluster": 1
    },
    "server": [
        {
            "id": "sample",
            "listen": "*",
            "port": 8090,
            "entry": "app.SampleRest",
            "enable": "local",
            "router": {
                "nnt.server.apidoc.Router": {
                    "export": {
                        "router": [
                            "entry://app/samplerest"
                        ],
                        "model": [
                            "entry://app/samplerest"
                        ]
                    }
                }
            }
        },
        {
            "id": "sample",
            "listen": "*",
            "port": 80,
            "entry": "app.SampleRest",
            "enable": "devops-develop,devops-release",
            "router": {
                "nnt.server.apidoc.Router": {
                    "export": {
                        "router": [
                            "entry://app/samplerest"
                        ],
                        "model": [
                            "entry://app/samplerest"
                        ]
                    }
                }
            }
        },
        {
            "id": "mmo",
            "attach": "sample",
            "entry": "app.SampleMmo",
            "mqsrv": "amqp"
        },
        {
            "id": "gendata",
            "entry": "nnt.server.XlsxConfig",
            "dir": "~/src/app/data",
            "output": "~/src/app/data/data.ts",
            "enable": null
        },
        {
            "id": "amqp",
            "host": "develop.91egame.com",
            "vhost": "sample",
            "user": "sample",
            "password": "sample",
            "entry": "nnt.server.Amqpmq",
            "channel": {
                "users.online": {
                    "type": "fanout",
                    "durable": false,
                    "longliving": true
                },
                "users": {
                    "type": "fanout",
                    "durable": true,
                    "longliving": true
                }
            },
            "enable": "debug"
        },
        {
            "id": "amqp",
            "host": "rabbitmq",
            "vhost": "sample",
            "user": "sample",
            "password": "sample",
            "entry": "nnt.server.Amqpmq",
            "channel": {
                "users.online": {
                    "type": "fanout",
                    "durable": false,
                    "longliving": true
                },
                "users": {
                    "type": "fanout",
                    "durable": true,
                    "longliving": true
                }
            },
            "enable": "distribution"
        }
    ],
    "dbms": [
        {
            "id": "memdb",
            "entry": "nnt.store.KvMemory"
        }
    ],
    "logger": [
        {
            "id": "console",
            "filter": "all",
            "entry": "nnt.logger.Console",
            "enable": "local"
        }
    ]
}
